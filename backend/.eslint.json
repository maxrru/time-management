{
    "extends": "standard",
    "env": {
        "jest": true
    },
    "rules": {
        "template-tag-spacing": 0,
        "semi": ["error", "never"],
        "no-unused-vars": ["error", "all"],
        "comma-dangle": ["error", {
            "arrays": "always-multiline",
            "objects": "always-multiline",
            "imports": "never",
            "exports": "never",
            "functions": "never"
        }]
    }
}
